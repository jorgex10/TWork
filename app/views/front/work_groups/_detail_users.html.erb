<div class="col-md-5">
<% if group.user_has_groups.blank? %>
  <div class="members">
    <br>
    No members has been found.
  </div>
<% else %>
  <div class="panel panel-info">
  <div class="panel-heading">Members</div>
  <div class="members">
    <table>
      <%= group.nameowner.name %>
      <% group.user_has_groups.each do |g| %>
        <tr>
          <td><%= g.show_member %></td>
          <td class="padding-td">.</td>
        </tr>
      <% end %>
    </table>
  </div>
  </div>
<% end %>
</div>

<div class="col-md-5">
<% if group.tasks.blank? %>
  <div class="members">
    <br>
    No task has been registered
  </div>
<% else %>
  <div class="panel panel-info">
  <div class="panel-heading">Task</div>
  <div class="members">
    <table>
      <% group.tasks.each do |t| %>
        <tr>
          <td><%= t.name %></td>
          <td class="padding-td">.</td>
        </tr>
      <% end %>
    </table>
  </div>
  </div>
<% end %>
</div>

<div class="col-md-2"></div>